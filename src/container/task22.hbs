<main>
    {{#with page_header}}
    <h1 class="text-center">{{title}}</h1>
    <p class="text-overcase text-center">{{description}}</p>
    {{/with}}
    {{#with section}}
    {{#with about_us}}
        <div class="card my-5">
            <div class="card-body">
                {{#with title_list}}
                    <div class="display-6 text-end">{{main}}</div>
                    <h2 class="text-end my-5">{{sub}}</h2>
                {{/with}}
                {{#with button}}<button class="btn btn-outline-info">{{text}}</button>{{/with}}
            </div>
        </div>
    {{/with}}
    {{#with advantage}}
        <div class="card">
            <div class="card-body">
                <div class="display-5">{{title}}</div>
                <div class="card my-5">
                    <div class="card-body">
                        {{#with efficiency}}
                            <div class="display-4 mb-4">{{title}}</div>
                            <h6>{{sub_title}}</h6>
                            <p>{{description}}</p>
                            <button class="btn btn-outline-warning">{{button}}</button>
                        {{/with}}
                    </div>
                </div>
                <div class="card">
                    <div class="card-body">
                        {{#with innovation}}
                        <div class="display-4 text-center mb-4">{{title}}</div>
                        <h6 class="text-center">{{sub_title}}</h6>
                        <p class="text-center">{{description}}</p>
                        <button class="btn btn-outline-warning">{{button}}</button>
                        {{/with}}
                    </div>
                </div>
                <div class="card my-5">
                    <div class="card-body">
                        {{#with collaboration}}
                        <div class="display-4 text-end mb-5">{{title}}</div>
                        <h6 class="text-end">{{sub_title}}</h6>
                        <p class="text-end">{{description}}</p>
                        <button class="btn btn-outline-warning">{{button}}</button>
                        {{/with}}
                    </div>
                </div>
            </div>
        </div>
    {{/with}}
    {{#with agreement}}
        <div class="card mt-5">
            <div class="card-body">
                <div class="display-5 mb-4">{{title}}</div>
                <div class="card">
                    <div class="card-body">
                        {{#with info}}
                        <h5 class="mb-4">{{sub_title}}</h5>
                        <div class="card">
                            <div class="card-body">
                                {{#with detail}}
                                <p>{{text}}</p>
                                <button class="btn btn-success">{{button}}</button>
                                {{/with}}
                            </div>
                        </div>
                        {{/with}}
                    </div>
                </div>
            </div>
        </div>
    {{/with}}
    {{/with}}
</main>