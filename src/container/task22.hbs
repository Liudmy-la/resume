<main>
    {{#with page_header}}
    <h1 class="text-center">{{title}}</h1>
    <p class="text-overcase text-center">{{description}}</p>
    {{/with}}
    <br>
    {{#with section}}
    {{#with about_us}}
        <div class="card">
            <div class="card-body">
                {{#with title_list}}
                    <div class="display-6 text-end">{{main}}</div>
                    <br>
                    <h2 class="text-end">{{sub}}</h2>
                    <br>
                {{/with}}
                {{#with button}}<button class="btn btn-outline-info">{{text}}</button>{{/with}}
            </div>
        </div>
    {{/with}}
    <br>
    {{#with advantage}}
        <div class="card">
            <div class="card-body">
                <div class="display-5">{{title}}</div>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with efficiency}}
                            <div class="display-4">{{title}}</div>
                            <br>
                            <h6>{{sub_title}}</h6>
                            <p>{{description}}</p>
                            <button class="btn btn-outline-warning">{{button}}</button>
                        {{/with}}
                    </div>
                </div>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with innovation}}
                        <div class="display-4 text-center">{{title}}</div>
                        <br>
                        <h6 class="text-center">{{sub_title}}</h6>
                        <p class="text-center">{{description}}</p>
                        <button class="btn btn-outline-warning">{{button}}</button>
                        {{/with}}
                    </div>
                </div>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with collaboration}}
                        <div class="display-4 text-end">{{title}}</div>
                        <br>
                        <h6 class="text-end">{{sub_title}}</h6>
                        <p class="text-end">{{description}}</p>
                        <button class="btn btn-outline-warning">{{button}}</button>
                        {{/with}}
                    </div>
                </div>
                <br>

            </div>
        </div>
    {{/with}}
    <br>
    {{#with agreement}}
        <div class="card">
            <div class="card-body">
                <div class="display-5">{{title}}</div>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with info}}
                        <h5>{{sub_title}}</h5>
                        <br>
                        <div class="card">
                            <div class="card-body">
                                {{#with detail}}
                                <p>{{text}}</p>
                                <button class="btn btn-success">{{button}}</button>
                                {{/with}}
                            </div>
                        </div>
                        {{/with}}
                    </div>
                </div>
            </div>
        </div>
    {{/with}}
    {{/with}}
</main>