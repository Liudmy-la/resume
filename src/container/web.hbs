<main>
{{#with web}}
{{> title data="Languages" big=true}}
{{#each languages}}
<hr class="my-5">
    {{> title data=name big=false}}
    <div><big>Version: {{version}}</big></div>
    <blockquote>{{description}}</blockquote> 
    {{#each elements}}    
    <ul class="mt-5">
        <li>
            <div>Element: <b>{{name}}</b></div>
            <div><i>{{description}}</i></div>
            <div class="mt-5">Attributes:
                {{#each attributes}}
                <ul>
                    <li><b>{{name}}</b> - {{description}}</li>
                </ul>
                {{/each}}
            </div>
        </li>
    </ul>
    {{/each}}
    {{#each properties}}    
    <ul>
        <li>
            <div>Property: <b>{{name}}</b></div>
            <div><i>{{description}}</i></div>
            <div class="mt-5">Values:
                {{#each values}}
                <ul>
                    <li><b>{{value}}</b> - {{description}}</li>
                </ul>
                {{/each}}
            </div>
        </li>
    </ul>
    {{/each}}    
    {{#each functions}}    
    <ul>
        <li>
            <div>Function: <b>{{name}}</b></div>
            <div><i>{{description}}</i></div>
            <div class="mt-5">Parameters:
                {{#each parameters}}
                <ul>
                    <li>                        
                        <div><b>{{name}}</b> - {{description}}</div>
                        <div><i>type:</i> {{type}}</div>                        
                    </li>
                </ul>
                {{/each}}
            </div>
        </li>
    </ul>
    {{/each}}
{{/each}}
{{/with}}
</main>